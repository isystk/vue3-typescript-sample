{
  "name": "vue3-typescript-sample",
  "description": "Vue3 の学習用サンプルアプリケーションです。",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "PORT=9000 vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src/*.{ts,vue} && eslint src/**/*.{ts,vue}",
    "lintfix": "eslint --fix src/*.{ts,vue} && eslint --fix src/**/*.{ts,vue}",
    "format": "prettier -w src/*.{ts,vue} && prettier -w src/**/*.{ts,vue}",
    "fix": "yarn format && yarn lint",
    "test": "jest",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy:production": "now -t $VERCEL_TOKEN -c --prod",
    "deploy:preview": "now -t $VERCEL_TOKEN -c"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "vue": "^3.2.36",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@mdi/font": "^6.7.96",
    "@storybook/addon-actions": "^6.5.4",
    "@storybook/addon-essentials": "^6.5.4",
    "@storybook/addon-links": "^6.5.4",
    "@storybook/vue3": "^6.5.4",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vuetify/vite-plugin": "^1.0.0-alpha.10",
    "@vueuse/head": "^0.7.6",
    "amazon-cognito-identity-js": "^5.2.9",
    "autoprefixer": "^10.4.7",
    "aws-sdk": "^2.1141.0",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.16.0",
    "eslint-plugin-vue": "^9.0.1",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "jsonc-parser": "^3.0.0",
    "lint-staged": "^12.4.1",
    "lodash": "^4.17.21",
    "now": "^21.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "storybook-builder-vite": "^0.1.23",
    "storybook-vue3-router": "^2.2.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.4",
    "vee-validate": "^4.5.11",
    "vite": "^2.9.9",
    "vite-plugin-eslint": "^1.6.1",
    "vite-plugin-pages": "^0.23.0",
    "vue-i18n": "^9.1.10",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.34.16",
    "vue3-jest": "^27.0.0-alpha.1",
    "vuejs-image-base64": "^1.2.0",
    "vuetify": "^3.0.0-beta.1",
    "yup": "^0.32.11"
  }
}
